{
  "name": "bentools/webpush-bundle",
  "description": "Send push notifications through Web Push Protocol to your Symfony users.",
  "license": "MIT",
  "authors": [
    {
      "name": "Beno!t POLASZEK",
      "email": "bpolaszek@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "minishlink/web-push": "^4.0",
    "symfony/http-kernel": "^3.0|^4.0"
  },
  "require-dev": {
    "bentools/doctrine-static": "1.0.x-dev",
    "nyholm/symfony-bundle-test": "^1.4",
    "phpunit/phpunit": "^5.0|^6.0|^7.0",
    "symfony/config": "^3.0|^4.0",
    "symfony/dependency-injection": "^3.0|^4.0",
    "symfony/framework-bundle": "^3.0|^4.0",
    "symfony/http-foundation": "^3.0|^4.0",
    "symfony/routing": "^3.0|^4.0",
    "symfony/security": "^3.0|^4.0",
    "symfony/var-dumper": "^3.0|^4.0",
    "symfony/yaml": "^3.0|^4.0",
    "twig/twig": "~1.0|~2.0"
  },
  "autoload": {
    "psr-4": {
      "BenTools\\WebPushBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\WebPushBundle\\Tests\\": "tests"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "keywords": [
    "symfony",
    "bundle",
    "flex",
    "push",
    "notification",
    "push notifications",
    "web push",
    "webpush",
    "subscription",
    "push subscription",
    "PushSubscription",
    "vapid",
    "fcm",
    "browser",
    "chrome",
    "firefox"
  ]
}
